{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client:dev": "nodemon --watch 'server/**/*.ts' --exec 'ts-node --project tsconfig.server.json' server/index.ts",
    "client:start": "cross-env NODE_ENV=production ts-node --project tsconfig.server.json server/index.ts",
    "client:build": "next build",
    "server:dev": "nodemon --watch 'serverApi/**/*.ts' --exec 'ts-node --project tsconfig.server.json' serverApi/index.ts",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\""
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.15.0",
    "@react-spring/parallax": "^9.0.0-rc.3",
    "axios": "^0.19.2",
    "chance": "^1.1.7",
    "dotenv-webpack": "^2.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.0",
    "next": "^9.5.2",
    "react": "16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "16.13.1",
    "react-spring": "^9.0.0-rc.3",
    "react-typed": "^1.2.0",
    "sass": "^1.26.10",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.1.1",
    "swr": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.45",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
